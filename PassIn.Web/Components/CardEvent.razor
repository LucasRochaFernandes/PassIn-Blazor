@using PassIn.Communication.Responses

<MudCard>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@EventJson!.Title</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.h6">@EventJson!.Details</MudText>
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
